{
  "extends": "@b1dx/typescript-config/react",
  "compilerOptions": {
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "incremental": true,
    "esModuleInterop": true,
    "jsx": "preserve"
  },
  "include": [
    "*.ts",
    "*.tsx",
    "app/**/*",
    "next-env.d.ts",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
