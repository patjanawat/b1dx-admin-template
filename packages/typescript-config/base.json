{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "lib": [
      "ES2022",
      "DOM",
      "DOM.Iterable"
    ],
    "strict": true,
    "noEmit": true,
    "skipLibCheck": true,
    "isolatedModules": true,
    "allowJs": true,
    "allowImportingTsExtensions": true,
    "useDefineForClassFields": false,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true
  }
}
